extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Exame #{exam._id}
    .w3-container
        p Index nº #{exam.index}
        p Data: #{exam.dataEMD}
        p Nome: #{exam.nome.primeiro} #{exam.nome.último}
        p Idade: #{exam.idade}
        p Género: #{exam.género}
        p Morada: #{exam.morada}
        p Modalidade: #{exam.modalidade}
        p Clube: #{exam.clube}
        p Email: #{exam.email}
        p Federado: #{exam.federado}
        p Resultado: #{exam.resultado}