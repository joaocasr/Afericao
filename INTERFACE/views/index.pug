extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Gestão de Exames
    .w3-container
      table.w3-table-all
        tr
          th dataEMD
          th Nome
          th Género
          th Idade
          th Resultado
        each exame in exames
          tr(onclick='showExam(\"'+ exame._id +'\")')
            td= exame.dataEMD
            td #{exame.nome.primeiro} #{exame.nome.último}
            td= exame.género
            td= exame.idade
            td= exame.resultado

